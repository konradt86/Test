from selenium import webdriver
from selenium.webdriver.edge.service import Service
from webdriver_manager.microsoft import EdgeChromiumDriverManager

# Inicjalizacja drivera Edge
driver = webdriver.Edge(service=Service(EdgeChromiumDriverManager().install()))

try:
    # Otwarcie URL
    url = 'http://example.com'
    driver.get(url)
    
    # Sprawdzenie, czy URL został otworzony
    if driver.current_url == url:
        print("URL został pomyślnie otworzony.")
    else:
        print("Nie udało się otworzyć URL.")
        
    # Sprawdzenie, czy driver został zainicjowany
    if driver.service.is_connectable():
        print("Driver został zainicjowany.")
    else:
        print("Driver nie został zainicjowany.")
finally:
    # Zamknięcie sesji przeglądarki
    driver.quit()
